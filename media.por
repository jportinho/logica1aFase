programa {
  funcao inicio() {
    real primeiroNumero, segundoNumero, mediaNumeros

    escreva("Digite o primeiro número: ")
    leia(primeiroNumero)

    escreva("Digite o segundo número: ")
    leia(segundoNumero)

    mediaNumeros = (primeiroNumero + segundoNumero) /2
    
    escreva("A média dos números é: " + mediaNumeros)

    
  }
}
